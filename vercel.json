{
  "crons": [
    {
      "path": "/api/recordings/process-transcription",
      "schedule": "0 0 * * *"
    }
  ],
  "headers": [
    {
      "source": "/api/files/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
